const players = ['Bernard', 'Youchi', 'Yenting', 'Angela', 'Yvonne', 'Ellen', 'Walter', 'Tim', 'Kevin', 'Russell']
const blackList = ['Walter', 'Tim']
// write your code
for(let i = 0; i < players.length; i++){
  if(blackList.includes(players[i])){
    players.splice(i,1)
    i --
  }
}
console.log(players) 
